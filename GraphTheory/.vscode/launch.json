    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - Dijkstra",
            "request": "launch",
            "mainClass": "mine.learn.graphtheory.Dijkstra",
            "projectName": "GraphTheory"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - MainFrame",
            "request": "launch",
            "mainClass": "mine.learn.graphtheory.gui.MainFrame",
            "projectName": "GraphTheory"
        },
        {
            "type": "java",
            "name": "Debug Current File",
            "request": "launch",
            "mainClass": "${file}",
            "console": "internalConsole",
            "stepFilters": {
                "classNameFilters": [
                    "java.*",
                    "javax.*",
                    "com.sun.*",
                    "sun.*",
                    "sunw.*",
                    "org.omg.*"
                ],
                "skipSynthetics": false,
                "skipStaticInitializers": false,
                "skipConstructors": false
            }
        }
    ]
}